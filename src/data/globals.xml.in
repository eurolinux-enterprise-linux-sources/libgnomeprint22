<?xml version="1.0"?>
<Globals>
  <Option Id="Globals">
    <Option Id="Document">
      <Option Id="Name" Type="String" Default=""/>
      <Option Id="PreferedUnit" Type="String" Default="cm"/>
      <Option Id="Page">
        <Option Id="LogicalOrientation" Type="List" Default="R0">
          <Item Id="R0">
            <_Name>Portrait</_Name>
            <Key Id="Page2LayoutTransform" Value="matrix(1 0 0 1 0 0)"/>
          </Item>
          <Item Id="R90">
            <_Name>Landscape</_Name>
            <Key Id="Page2LayoutTransform" Value="matrix(0 1 -1 0 0 1)"/>
          </Item>
          <Item Id="R180">
            <_Name>Upside down portrait</_Name>
            <Key Id="Page2LayoutTransform" Value="matrix(-1 0 0 -1 1 1)"/>
          </Item>
          <Item Id="R270">
            <_Name>Upside down landscape</_Name>
            <Key Id="Page2LayoutTransform" Value="matrix(0 -1 1 0 1 0)"/>
          </Item>
        </Option>
        <Option Id="Margins">
          <Option Id="Bottom" Type="String" Default="3 cm"/>
          <Option Id="Top" Type="String" Default="3 cm"/>
          <Option Id="Right" Type="String" Default="2 cm"/>
          <Option Id="Left" Type="String" Default="2 cm"/>
        </Option>
        <Option Id="Layout" Type="List" Default="Plain">
          <Item Id="Plain">
            <_Name>Plain</_Name>
            <Key Id="ValidPhysicalSizes" Value="All"/>
            <Key Id="LogicalPages" Value="1"/>
            <Key Id="PhysicalPages" Value="1"/>
            <Key Id="Width" Value="1"/>
            <Key Id="Height" Value="1"/>
            <Key Id="Pages" Value="LP0">
              <Key Id="LP0" Value="transform(1 0 0 1 0 0)"/>
            </Key>
          </Item>
          <Item Id="2_1">
            <_Name>2 pages to 1</_Name>
            <Key Id="ValidPhysicalSizes" Value="All"/>
            <Key Id="LogicalPages" Value="2"/>
            <Key Id="PhysicalPages" Value="1"/>
            <Key Id="Width" Value="0.5"/>
            <Key Id="Height" Value="1"/>
            <Key Id="Pages" Value="LP0">
              <Key Id="LP0" Value="transform(0 -SQRT1_2 SQRT1_2 0 0 1)"/>
              <Key Id="LP1" Value="transform(0 -SQRT1_2 SQRT1_2 0 0 0.5)"/>
            </Key>
          </Item>
          <Item Id="4_1">
            <_Name>4 pages to 1</_Name>
            <Key Id="ValidPhysicalSizes" Value="All"/>
            <Key Id="LogicalPages" Value="4"/>
            <Key Id="PhysicalPages" Value="1"/>
            <Key Id="Width" Value="0.5"/>
            <Key Id="Height" Value="0.5"/>
            <Key Id="Pages" Value="LP0">
              <Key Id="LP0" Value="transform(0.5 0 0 0.5 0 0.5)"/>
              <Key Id="LP1" Value="transform(0.5 0 0 0.5 0.5 0.5)"/>
              <Key Id="LP2" Value="transform(0.5 0 0 0.5 0 0)"/>
              <Key Id="LP3" Value="transform(0.5 0 0 0.5 0.5 0)"/>
            </Key>
          </Item>
          <Item Id="I2_1">
            <_Name>Divided</_Name>
            <Key Id="ValidPhysicalSizes" Value="All"/>
            <Key Id="LogicalPages" Value="2"/>
            <Key Id="PhysicalPages" Value="1"/>
            <Key Id="Width" Value="0.5"/>
            <Key Id="Height" Value="1"/>
            <Key Id="Pages" Value="LP0">
              <Key Id="LP0" Value="transform(0 -1 1 0 0 1)"/>
              <Key Id="LP1" Value="transform(0 -1 1 0 0 0.5)"/>
            </Key>
          </Item>
          <Item Id="IM2_1">
            <_Name>Folded</_Name>
            <Key Id="ValidPhysicalSizes" Value="All"/>
            <Key Id="LogicalPages" Value="2"/>
            <Key Id="PhysicalPages" Value="1"/>
            <Key Id="Width" Value="0.5"/>
            <Key Id="Height" Value="1"/>
            <Key Id="Pages" Value="LP0">
              <Key Id="LP0" Value="transform(0 -1 1 0 0 0.5)"/>
              <Key Id="LP1" Value="transform(0 1 -1 0 1 0.5)"/>
            </Key>
          </Item>
        </Option>
      </Option>
    </Option>
